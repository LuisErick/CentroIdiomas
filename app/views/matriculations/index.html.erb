<h1>Listing matriculations</h1>

<table>
  <thead>
    <tr>
      <th>Language</th>
      <th>Student</th>
      <th>Administrative</th>
      <th>Course type</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matriculations.each do |matriculation| %>
      <tr>
        <td><%= matriculation.language_id %></td>
        <td><%= matriculation.student_id %></td>
        <td><%= matriculation.administrative_id %></td>
        <td><%= matriculation.course_type_id %></td>
        <td><%= matriculation.date %></td>
        <td><%= link_to 'Show', matriculation %></td>
        <td><%= link_to 'Edit', edit_matriculation_path(matriculation) %></td>
        <td><%= link_to 'Destroy', matriculation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Matriculation', new_matriculation_path %>
