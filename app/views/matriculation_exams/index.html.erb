<h1>Listing matriculation_exams</h1>

<table>
  <thead>
    <tr>
      <th>Matriculation</th>
      <th>Teacher</th>
      <th>Institution</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matriculation_exams.each do |matriculation_exam| %>
      <tr>
        <td><%= matriculation_exam.matriculation_id %></td>
        <td><%= matriculation_exam.teacher_id %></td>
        <td><%= matriculation_exam.institution %></td>
        <td><%= link_to 'Show', matriculation_exam %></td>
        <td><%= link_to 'Edit', edit_matriculation_exam_path(matriculation_exam) %></td>
        <td><%= link_to 'Destroy', matriculation_exam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Matriculation exam', new_matriculation_exam_path %>
