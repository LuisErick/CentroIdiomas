<h1>Listing unica_students</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Faculty</th>
      <th>Grade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @unica_students.each do |unica_student| %>
      <tr>
        <td><%= unica_student.student_id %></td>
        <td><%= unica_student.faculty_id %></td>
        <td><%= unica_student.grade_id %></td>
        <td><%= link_to 'Show', unica_student %></td>
        <td><%= link_to 'Edit', edit_unica_student_path(unica_student) %></td>
        <td><%= link_to 'Destroy', unica_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Unica student', new_unica_student_path %>
